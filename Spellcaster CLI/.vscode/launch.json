{
  "version": "2.0.0",
  "tasks": [
      {
          "label": "Build",
          "type": "process",
          "command": "dotnet",
          "args": [
              "build"
          ],
          "group": {
              "kind": "build",
              "isDefault": true
          },
          "problemMatcher": "$msCompile"
      },
      {
          "label": "Run",
          "type": "process",
          "command": "dotnet",
          "args": [
              "run"
          ],
          "group": {
              "kind": "build",
              "isDefault": false
          },
          "problemMatcher": "$msCompile"
      }
  ]
}
